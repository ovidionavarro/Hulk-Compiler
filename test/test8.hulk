function mean(numbers: Number): Number =>
    let total = 0 in {
        for (x in numbers)
            total := total + x;

        total / size(numbers);
    };

let numbers = [1,2,3,4,5] in
    print(mean(numbers));


function fib(n) => if (n == 0 | n == 1) 1 else fib(n-1) + fib(n-2);
