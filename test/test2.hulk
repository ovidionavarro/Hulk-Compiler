
type Numbaaer {
    x = 0;
    y = 0;

    getX() => self.x;
    getx() => self.y;

    setX(x) => self.x := x;
    setY(y) => self.y := y;
}

type Point {
    x = 0;
    y = 0;

    getX() => self.x;
    getY() => self.y;

    setX(x) => self.x := x;
    setY(y) => self.y := y;
}

type PolarPoint inherits Point {
    y = 0;
    rho() => sqrt(self.getX() ^ 2 + self.getY() ^ 2);
    getX():Boolean => self.x;
    getY():Boolean => self.y;
}
